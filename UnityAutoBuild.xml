<?xml version="1.0" encoding="utf-8"?>
<UnityAutoBuild
  xmlns="http://schema.fixstudio.pro/UnityAutoBuild/v1">

  <FetchSpec>master</FetchSpec>
  <ProjectPath></ProjectPath>
  <Targets>
    <Config id="win" base="WIN_X86" out="out.win_x86">
      <Bundler>ZIP</Bundler>
    </Config>
    <Config id="linux" base="LINUX" out="out.linux">
      <Bundler>TAR_XZ</Bundler>
    </Config>
    <Config id="server" base="LINUX_HEADLESS" out="out.server">
      <Bundler>TAR_XZ</Bundler>
    </Config>
    <Config id="android" base="ANDROID" out="out.apk" />

    <!--
    <Config name="android_debug" base="ANDROID" out="out.debug.apk">
      <Prop name="Development" value="true" />
    </Config>
    -->
  </Targets>
</UnityAutoBuild>
